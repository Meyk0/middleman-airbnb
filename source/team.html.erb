<div class="team">
  <ul class="list-inline text-center">
    <% data.users.each do |user| %>
      <li>
        <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.gh_username}", class: "img-circle" %>
        <h2><%= user.name.capitalize %></h2>
          <p>
            <i class="fa fa-github"></i>
            <%= link_to "#{user.gh_username}", "https://github.com/#{user.gh_username}", class: "github-link"%>
          </p>
        </h3>
      </li>
    <% end %>
  </ul>
</div>
